<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/activityBackground"
    tools:context=".AboutUsActivity">

    <TextView
        android:id="@+id/aboutus_application"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="21dp"
        android:layout_marginEnd="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginRight="33dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="16dp"
        android:maxLines="25"
        android:text="TextView"
        android:textColor="#000d23"
        app:layout_constraintBottom_toTopOf="@+id/aboutus_version"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/aboutus_version"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="32dp"
        android:layout_marginStart="32dp"
        android:textColor="#000d23"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/aboutus_contactnumber"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aboutus_application" />

    <TextView
        android:id="@+id/aboutus_contactnumber"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="17dp"
        android:text="TextView"
        android:textColor="#000d23"
        app:layout_constraintBottom_toTopOf="@+id/aboutus_email"
        app:layout_constraintEnd_toEndOf="@+id/aboutus_version"
        app:layout_constraintStart_toStartOf="@+id/aboutus_email"
        app:layout_constraintTop_toBottomOf="@+id/aboutus_version" />

    <TextView
        android:id="@+id/aboutus_email"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="232dp"
        android:layout_marginEnd="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginRight="33dp"
        android:layout_marginStart="33dp"
        android:textColor="#000d23"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aboutus_contactnumber" />


</android.support.constraint.ConstraintLayout>